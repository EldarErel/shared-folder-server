{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "File",
  "description": "File create scheme",
  "type": "object",
  "properties": {
    "name": {
      "description": "The unique name of the file",
      "type": "string",
      "minLength": 1
    },
    "content": {
      "description": "content",
      "type": "string"
    }
  },
  "required": ["name", "content"]
}